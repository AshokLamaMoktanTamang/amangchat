{
  "name": "@amangchat/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --parallel --target=serve --projects=amangchat-api-gateway,amangchat-user-service,amangchat-chat-service",
    "prepare": "husky install",
    "start:apigateway": "nx run amangchat-api-gateway:serve",
    "build:apigateway": "nx run amangchat-api-gateway:build",
    "start:userService": "nx run amangchat-user-service:serve",
    "build:userService": "nx run amangchat-user-service:build",
    "start:chatService": "nx run amangchat-chat-service:serve",
    "build:chatService": "nx run amangchat-chat-service:build",
    "generate:protobuf": "mkdir -p libs/common/protobuf && ./node_modules/.bin/grpc_tools_node_protoc --grpc_out='grpc_js:libs/common/protobuf' --js_out=import_style=commonjs,binary:libs/common/protobuf --ts_out='grpc_js:libs/common/protobuf' --proto_path libs/common/protos libs/common/protos/*.proto"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/mongoose": "^10.0.5",
    "@nestjs/platform-express": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "crypto": "^1.0.1",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "husky": "^9.0.11",
    "mongoose": "^8.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "18.2.4",
    "@nx/eslint-plugin": "18.2.4",
    "@nx/jest": "18.2.4",
    "@nx/js": "18.2.4",
    "@nx/nest": "18.2.4",
    "@nx/node": "18.2.4",
    "@nx/web": "18.2.4",
    "@nx/webpack": "18.2.4",
    "@nx/workspace": "18.2.4",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "18.2.4",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.4.2",
    "webpack-cli": "^5.1.4"
  }
}
